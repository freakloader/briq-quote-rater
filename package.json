    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.5.8",
    "nodemon": "^2.0.4"
  },
  "engines": {
    "node": "12.13.0",
    "npm": "6.14.3"
  },
  "devDependencies": {
	"@google-cloud/language": "^4.0.1",
    "concurrently": "^5.2.0"
  }
}
